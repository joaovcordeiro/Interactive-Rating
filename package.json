{
  "devDependencies": {
    "@parcel/transformer-sass": "^2.7.0",
    "gh-pages": "^4.0.0",
    "parcel": "^2.7.0",
    "parcel-reporter-static-files-copy": "^1.4.0"
  },
  "scripts": {
    "start": "parcel index.html",
    "prebuild-dev": "rm -rf dist",
    "build-dev": "parcel build index.html --no-minify --no-source-maps",
    "prebuild-prod": "rm -rf dist",
    "build-prod": "parcel build index.html",
    "deploy": "gh-pages -d dist"
  },
  "staticFiles": {
    "staticPath": "images"
  }
}
